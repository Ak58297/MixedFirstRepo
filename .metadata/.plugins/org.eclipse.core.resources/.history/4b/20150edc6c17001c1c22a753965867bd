

public class Sorting {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int arr[]= {2,5,7,90,2,1,0};
		
		for(int i=0;i<arr.length;i++)
		{
			if (arr[i]<arr[i+1])
			
			{
				int temp=arr[i];
				arr[i]=arr[i+1];
				arr[i+1]=temp;
						
			}
			
		}
		for(int i=0;i<arr.length;i++)
		{
			System.out.println(arr[i]);
		}
		
		
	}

}
