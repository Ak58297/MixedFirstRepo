import java.util.HashMap;

public class Hash_Map {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		HashMap<Integer,String> hm= new HashMap<Integer,String> ();
		hm.put(0, "First");
		hm.put(1, "Second");
		hm.put(2, "Third");
		hm.put(3, "First");
		hm.put(4, "First");
		hm.put(5, "First");
		System.out.println(hm);
	}

}
